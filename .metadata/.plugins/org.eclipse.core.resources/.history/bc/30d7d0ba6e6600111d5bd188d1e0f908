package controller;

public class DesignPage {
	private StringBuilder page= new StringBuilder("");
	DesignPage(String str){
		page.append(str);
	}
	DesignPage(){
		page.append("<html>");
		page.append("<body>");
	}
	public void pageCon(String str){
		page.append(str);
	}
public String getPage(){
		page.append("</body>");
		page.append("</html>");
		return page.toString();
	}
}
