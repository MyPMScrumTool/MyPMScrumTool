package controller;

public class DesignPage {
	private StringBuilder page;
	DesignPage(String str){
		page.append(str);
	}
	DesignPage(){
		page.append("<HTML>");
		page.append("<body>");
	}
	
	public String getPage(){
		page.append("</body>");
		page.append("</HTML>");
		return page.toString();
	}
}
